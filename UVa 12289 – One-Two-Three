#include <bits/stdc++.h>

using namespace std;



int main( )
{
    int u = 0,l = 1, t;

    vector <char> v;

    bool f;

    string s;

    cin>>t;

    while(t--)
    {
        cin>>s;

        if(s.size() == 5)
            cout<<"3"<<endl;
        else
        {
            if((s[0] == 'o' && s[1] == 'n') || (s[0] == 'o' && s[2] == 'e') || (s[1] == 'n' && s[2] == 'e'))
                cout<<"1"<<endl;
            else
                cout<<"2"<<endl;
        }
    }
}
