#include <bits/stdc++.h>

using namespace std;



int main( )
{
    int  a;

    vector <char> v;

    string s;

    while(getline(cin, s))
    {
        for(int i = 0; i < s.size(); i++)
        {
            if(s[i] >= 'A' && s[i] <= 'C')
                v.push_back('2');

            else if(s[i] >= 'D' && s[i] <= 'F')
                v.push_back('3');

            else if(s[i] >= 'G' && s[i] <= 'I')
                v.push_back('4');

            else if(s[i] >= 'J' && s[i] <= 'L')
                v.push_back('5');

            else if(s[i] >= 'M' && s[i] <= 'O')
                v.push_back('6');

            else if(s[i] >= 'P' && s[i] <= 'S')
                v.push_back('7');

            else if(s[i] >= 'T' && s[i] <= 'V')
                v.push_back('8');

            else if(s[i] >= 'W' && s[i] <= 'Z')
                v.push_back('9');

            else
                v.push_back(s[i]);


        }

        for(int i = 0; i < v.size(); i++)
            cout<<v[i];

        cout<<endl;

        v.clear();
    }
}




