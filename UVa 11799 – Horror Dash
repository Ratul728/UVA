#include<bits/stdc++.h>

using namespace std;

int main()
{

    int t,a,s,k,m=1;

    cin>>t;

    while(t--)
    {
        cin>>k;

        s = -1;

        while(k--)
        {
            cin>>a;

            if(a > s)
                s = a;
        }

        printf("Case %d: %d\n",m,s);
        m++;


    }

    return 0;

}
