#include <bits/stdc++.h>

using namespace std;

int main( )
{
    int t, a, b,c, m = 1;

    cin>>t;

    while(t--)
    {
        cin>>a>>b>>c;

        if(a > 20 || b > 20 || c > 20)
            printf("Case %d: bad\n", m);
        else
            printf("Case %d: good\n", m);

        m++;
    }
}
