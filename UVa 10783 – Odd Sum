#include<bits/stdc++.h>

using namespace std;

int main()
{

    int t,a,s,b,m=1;

    cin>>t;

    while(t--)
    {
        cin>>a>>b;

        if(a % 2 == 0)
            a++;

        s = 0;

        for(int i = a; i <= b; i += 2)
            s += i;

        printf("Case %d: %d\n",m,s);
        m++;


    }

    return 0;

}
