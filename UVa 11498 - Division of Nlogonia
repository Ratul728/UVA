#include <iostream>

using namespace std;

int main()
{
    int t,a,b,m,n;

    while(1)
    {
        cin>>t;

        if(t == 0) break;

        cin>>m>>n;

        while(t--)
        {
            cin>>a>>b;

            if(a == m || b == n)
                cout<<"divisa"<<endl;

            else if(a > m && b > n)
                cout<<"NE"<<endl;

            else if(a > m && b < n)
                cout<<"SE"<<endl;

            else if(a < m && b > n)
                cout<<"NO"<<endl;

            else if(a < m && b < n)
                cout<<"SO"<<endl;
        }
    }






    return 0;

}
